{
  "trigger_phrases": {
    "implementation_mode": [
      "yert"
    ],
    "discussion_mode": [
      "SILENCE",
      "STOP"
    ],
    "task_creation": [
      "mek:",
      "make:"
    ],
    "task_startup": [
      "start^",
      "start:"
    ],
    "task_completion": [
      "finito",
      "end it"
    ],
    "context_compaction": [
      "squish"
    ]
  },
  "blocked_actions": {
    "implementation_only_tools": [
      "Write",
      "Edit",
      "MultiEdit",
      "NotebookEdit"
    ],
    "bash_read_patterns": [],
    "bash_write_patterns": [],
    "extrasafe": true
  },
  "git_preferences": {
    "add_pattern": "all",
    "default_branch": "main",
    "commit_style": "detailed",
    "auto_merge": true,
    "auto_push": true,
    "has_submodules": false
  },
  "environment": {
    "os": "macos",
    "shell": "zsh",
    "developer_name": "Mautasim"
  },
  "features": {
    "branch_enforcement": true,
    "task_detection": true,
    "auto_ultrathink": true,
    "icon_style": "emoji",
    "context_warnings": {
      "warn_85": true,
      "warn_90": true
    }
  }
